{
    "name": "mr-luke/framekit",
    "description": "Event Sourcing package",
    "keywords": ["event-sourcing", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Łukasz Sitnicki",
            "email": "lukasz.sitnicki@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^5.7",
        "mr-luke/configuration": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Mrluke\\Framekit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
