{
    "name": "mr-luke/framekit",
    "description": "Event Sourcing package",
    "keywords": ["event-sourcing", "laravel"],
    "type": "library",
    "licence": "MIT",
    "authors": [
        {
            "name": "Łukasz Sitnicki",
            "email": "lukasz.sitnicki@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "laravel/framework": "^8.0|^7.0|^6.0|^5.7",
        "mr-luke/configuration": "^1.0",
        "nesbot/carbon": "^2.16.0",
        "mr-luke/bus": "^1.0@beta"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "orchestra/testbench": "^6.0|^5.0|^4.0|^3.7.0",
        "phpunit/phpunit": "^8.0|^7.0",
        "pdepend/pdepend": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Framekit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Framekit\\Providers\\FramekitServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
