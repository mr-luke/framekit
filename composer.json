{
    "name": "mr-luke/framekit",
    "description": "Event Sourcing package",
    "keywords": ["event-sourcing", "laravel"],
    "type": "library",
    "licence": "MIT",
    "authors": [
        {
            "name": "≈Åukasz Sitnicki",
            "email": "lukasz.sitnicki@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "laravel/framework": "^10.0|^9.0|^8.0",
        "mr-luke/configuration": "^1.0",
        "nesbot/carbon": "^2.61",
        "mr-luke/bus": "^1.0@beta"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5",
        "pdepend/pdepend": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Framekit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Framekit\\Providers\\FramekitServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
